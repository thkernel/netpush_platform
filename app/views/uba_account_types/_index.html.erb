 <% @uba_account_types.each do |uba_account_type| %>
      <tr>
        <td><%= uba_account_type.name %></td>
         <td>
            <div class="action-buttons">
                    <%= link_to raw('<i class="fa fa-pencil" aria-hidden="true"></i>
'), edit_uba_account_type_path(uba_account_type.uid), title: "Modifier", remote: true %>




<%= link_to raw('<i class="fa fa-trash-o" aria-hidden="true"></i>
'), uba_account_type_delete_path(uba_account_type), remote: true, title: "Supprimer", class: "delete" %>
</div>
        </td>
      </tr>
    <% end %>