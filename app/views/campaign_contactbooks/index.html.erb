<p id="notice"><%= notice %></p>

<h1>Campaign Contactbooks</h1>

<table>
  <thead>
    <tr>
      <th>Uuid</th>
      <th>Contact book</th>
      <th>Campaign</th>
      <th>Status</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @campaign_contactbooks.each do |campaign_contactbook| %>
      <tr>
        <td><%= campaign_contactbook.uuid %></td>
        <td><%= campaign_contactbook.contact_book %></td>
        <td><%= campaign_contactbook.campaign %></td>
        <td><%= campaign_contactbook.status %></td>
        <td><%= campaign_contactbook.user %></td>
        <td><%= link_to 'Show', campaign_contactbook %></td>
        <td><%= link_to 'Edit', edit_campaign_contactbook_path(campaign_contactbook) %></td>
        <td><%= link_to 'Destroy', campaign_contactbook, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Campaign Contactbook', new_campaign_contactbook_path %>
